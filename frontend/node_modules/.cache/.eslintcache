 {\n          // Decode the JWT token\n          const decodedToken = JSON.parse(atob(token.split('.')[1]));\n          const userEmail = decodedToken.sub; // 'sub' claim typically contains the user identifier (email in this case)\n\n          dispatch({\n            type: 'SET_USER',\n            user: { email: userEmail }\n          });\n        } catch (error) {\n          console.error('Error decoding token:', error);\n          localStorage.removeItem('token'); // Remove invalid token\n          dispatch({\n            type: 'SET_USER',\n            user: null\n          });\n        }\n      } else {\n        dispatch({\n          type: 'SET_USER',\n          user: null\n        });\n      }\n    };\n\n    checkUserStatus();\n\n    // Add event listener for storage changes\n    window.addEventListener('storage', checkUserStatus);\n\n    // Cleanup function\n    return () => {\n      window.removeEventListener('storage', checkUserStatus);\n    };\n  }, [dispatch]);\n\n  return (\n    <Router>\n      <div className=\"app\">\n        <Routes>\n        <Route path=\"/orders\" element={<React.Fragment> \n            <Header />\n            <Orders />\n          </React.Fragment>} />\n          <Route path=\"/login\" element={<React.Fragment> \n            <Login />\n          </React.Fragment>} />\n          <Route path=\"/checkout\" element={<React.Fragment> \n            <Header />\n            <Checkout />\n          </React.Fragment>} />\n          <Route path=\"/payment\" element={<React.Fragment> \n            <Header />\n            <Elements stripe={promise}>\n              <Payment />\n            </Elements>\n          </React.Fragment>} />\n          <Route path=\"/\" element={<React.Fragment> \n            <Header />\n            <Home />\n            <Footer />\n          </React.Fragment>} />\n        </Routes>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/app/src/pages/reportWebVitals.js",[],"/app/src/pages/reducer.js",["88"],"export const initialState = {\n    basket: [],\n    user: null\n};\n\n//Selector\nexport const getBasketTotal = (basket) =>\n  basket?.reduce((amount, item) => item.price + amount, 0);\n\nconst reducer = (state, action) => { // state is used to hold the current state of the application\n    console.log(action);\n    switch(action.type){\n        case 'ADD_TO_BASKET':\n            return {\n                ...state, // uses the spread operator (...) to create a new state object that retains the existing properties of the state while modifying specific properties\n                basket: [...state.basket, action.item],\n            };\n\n        case 'EMPTY_BASKET':\n            return {\n              ...state,\n              basket: []\n            };\n\n        case 'REMOVE_FROM_BASKET':\n            const index = state.basket.findIndex(\n                (basketItem) => basketItem.id === action.id // Find the first one and return \n            );\n            let newBasket = [...state.basket];\n            if (index >= 0){\n                newBasket.splice(index, 1); // go to the index and cut it by one \n            } else{\n                console.warn('Cant remove product(id: ${action.id}) as its not in the basket!')\n            }\n            \n            return {\n                ...state, \n                basket: newBasket\n            }\n        case 'SET_USER':\n            return {\n                ...state, \n                user: action.user // the user we dispatch\n            }\n\n            default:\n            return state;\n    }\n}\n\n\nexport default reducer;","/app/src/pages/StateProvider.js",[],"/app/src/pages/Login.js",[],"/app/src/pages/Home.js",["89"],"import React from 'react'\nimport \"../styles/Home.css\"\nimport Product from './Product'\nimport { Carousel } from 'react-responsive-carousel';\nimport BannerCarousel from '../components/BannerCarousel';\nimport 'react-responsive-carousel/lib/styles/carousel.min.css';\n\nfunction Home() {\n 